		qcom,fg-esr-timer-asleep = <256 256>;
		qcom,fg-esr-timer-charging = <0 96>;
		qcom,cycle-counter-en;
		qcom,hold-soc-while-full;
		qcom,fg-auto-recharge-soc;
		qcom,fg-recharge-soc-thr = <98>;

		qcom,term-current-ma = <100>;
		status = "okay";

		// Motorola potter
		// #address-cells = <1>;
