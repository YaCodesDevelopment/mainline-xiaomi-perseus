/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/regulator/qcom,rpmh-regulator.h>
#include "sdm845.dtsi"

/ {
	model = "Xiaomi Technologies, Inc.";
	compatible = "qcom,sdm845-mtp", "qcom,sdm845";
	qcom,msm-id = <321 0x20001>;
	qcom,board-id = <99 0>;

	aliases {
		serial0 = &uart9;
		display0 = &framebuffer0;
	};

	chosen {
			#address-cells = <2>;
		#size-cells = <2>;
		ranges;

		/* Add "earlycon" intended to be used in combination with UART serial console */
		bootargs = "earlycon";

		/*
		 * Define as "serial0:115200n8" instead of simplefb "display0" for UART console;
		 * the tiny golden UART TX pad on the OnePlus 5 mainboard can be found in the
		 * bottom left corner right above the black screw.
		 */
		stdout-path = "display0";

		/*
		 * HACK: Use display framebuffer setup by the UEFI XBL bootloader for simplefb.
		 * the address was extracted from a bootloader dd image using the following:
		 * $ strings xbl.img | grep 'Display Reserved'
		 */
		framebuffer0: framebuffer@9d400000 {
			compatible = "simple-framebuffer";
			reg = <0 0x9d400000 0 (1080 * 2340 * 4)>;
			width = <1080>;
			height = <2340>;
			stride = <(1080 * 4)>;
			format = "a8r8g8b8";
			stdout-path = "display0";
		};
	};

	reserved-memory {
		#address-cells = <0x02>;
		#size-cells = <0x02>;
		ranges;

		hyp_region@85700000 {
			no-map;
			reg = <0 0x85700000 0 0x600000>;
		};

		xbl_region@85e00000 {
			no-map;
			reg = <0 0x85d00000 0 0x200000>;
		};

		removed_region@85fc0000 {
			no-map;
			reg = <0 0x85fc0000 0 0x4940000>;
		};

		qseecom_region@0x8ab00000 {
			compatible = "shared-dma-pool";
			no-map;
			reg = <0 0x8ab00000 0 0x1400000>;
		};

		camera_region@0x8bf00000 {
			compatible = "removed-dma-pool";
			no-map;
			reg = <0 0x8bf00000 0 0x500000>;
		};

		ips_fw_region@0x8c400000 {
			compatible = "removed-dma-pool";
			no-map;
			reg = <0 0x8c400000 0 0x10000>;
		};

		ipa_gsi_region@0x8c410000 {
			compatible = "removed-dma-pool";
			no-map;
			reg = <0 0x8c410000 0 0x5000>;
		};

		gpu_region@0x8c415000 {
			compatible = "removed-dma-pool";
			no-map;
			reg = <0 0x8c415000 0 0x2000>;
		};

		adsp_region@0x8c500000 {
			compatible = "removed-dma-pool";
			no-map;
			reg = <0 0x8c500000 0 0x1e00000>;
		};

		wlan_fw_region@0x8e300000 {
			compatible = "removed-dma-pool";
			no-map;
			reg = <0 0x8e300000 0 0x100000>;
		};

		modem_region@0x8e400000 {
			compatible = "removed-dma-pool";
			no-map;
			reg = <0 0x8e400000 0 0x7800000>;
		};

		video_region@0x95c00000 {
			compatible = "removed-dma-pool";
			no-map;
			reg = <0 0x95c00000 0 0x500000>;
		};

		cdsp_region@0x96100000 {
			compatible = "removed-dma-pool";
			no-map;
			reg = <0 0x96100000 0 0x800000>;
		};

		mba_region@0x96900000 {
			compatible = "removed-dma-pool";
			no-map;
			reg = <0 0x96900000 0 0x200000>;
		};

		slpi_region@0x96b00000 {
			compatible = "removed-dma-pool";
			no-map;
			reg = <0 0x96b00000 0 0x1400000>;
		};

		pil_spss_region@0x97f00000 {
			compatible = "removed-dma-pool";
			no-map;
			reg = <0 0x97f00000 0 0x100000>;
		};

		adsp_region {
			compatible = "shared-dma-pool";
			alloc-ranges = <0 0 0 0xffffffff>;
			reusable;
			alignment = <0 0x400000>;
			size = <0 0x1000000>;
		};

		qseecom_ta_region {
			compatible = "shared-dma-pool";
			alloc-ranges = <0 0 0 0xffffffff>;
			reusable;
			alignment = <0 0x400000>;
			size = <0 0x1000000>;
		};

		secure_sp_region {
			compatible = "shared-dma-pool";
			alloc-ranges = <0 0 0 0xffffffff>;
			reusable;
			alignment = <0 0x400000>;
			size = <0 0x800000>;
		};

		fb_mem@9d400000 {
			reg = <0 0x9d400000 0 0x2400000>;
			label = "cont_splash_region";
		};

		secure_display_region {
			compatible = "shared-dma-pool";
			alloc-ranges = <0 0 0 0xffffffff>;
			reusable;
			alignment = <0 0x400000>;
			size = <0 0x5c00000>;
		};

		mem_dump_region {
			compatible = "shared-dma-pool";
			reusable;
			size = <0 0x2400000>;
		};

		linux,cma {
			compatible = "shared-dma-pool";
			alloc-ranges = <0 0 0 0xffffffff>;
			reusable;
			alignment = <0 0x400000>;
			size = <0 0x2000000>;
			linux,cma-default;
		};

		ramdump_fb_region@af000000 {
			compatible = "removed-dma-pool";
			no-map;
			reg = <0 0xaf000000 0 0x1000000>;
		};
	};
	
	

	vph_pwr: vph-pwr-regulator {
		compatible = "regulator-fixed";
		regulator-name = "vph_pwr";
		regulator-min-microvolt = <3700000>;
		regulator-max-microvolt = <3700000>;
	};

	/*
	 * Apparently RPMh does not provide support for PM8998 S4 because it
	 * is always-on; model it as a fixed regulator.
	 */
	vreg_s4a_1p8: pm8998-smps4 {
		compatible = "regulator-fixed";
		regulator-name = "vreg_s4a_1p8";

		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;

		regulator-always-on;
		regulator-boot-on;

		vin-supply = <&vph_pwr>;
	};
};

&adsp_pas {
	status = "okay";
	firmware-name = "qcom/sdm845/adsp.mdt";
};

